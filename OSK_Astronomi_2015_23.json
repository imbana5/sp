    "question": [
        {
            "text": "Bila diketahui bahwa vektor $\\overrightarrow{A} = 2i + j - k$ dan vektor $\\overrightarrow{B} = i -2j + 2k$, maka pernyataan yang benar adalah"
        },
        {
            "start": "ol"
        },
        {
            "li": "$\\overrightarrow{A} + \\overrightarrow{B} = 3i - j + k$"
        },
        {
            "li": "$\\overrightarrow{A} \\cdot \\overrightarrow{B} = 2i - 2j - 2k$"
        },
        {
            "li": "$\\overrightarrow{C} = \\overrightarrow{A} \\times \\overrightarrow{B}$ berada di bidang $yz$"
        },
        {
            "li": "$\\overrightarrow{A}$ dan $\\overrightarrow{B}$ membentuk sudut $< 60^{\\circ}$."
        },
        {
            "end": "ol"
        }
    ],
    "choices": [
        {
            "value": "0",
            "label": "1, 2, dan 3 benar"
        },
        {
            "value": "1",
            "label": "1 dan 3 benar"
        },
        {
            "value": "2",
            "label": "2 dan 4 benar"
        },
        {
            "value": "3",
            "label": "hanya 4 benar"
        },
        {
            "value": "4",
            "label": "semua benar atau semua salah"
        }
    ],
    "answer": "1",
    "explanation": [
        {
            "text": "\\begin{eqnarray*}"
        },
        {
            "text": "\\overrightarrow{A} \\cdot \\overrightarrow{B} &=& (2 \\cdot 1) + (1 \\cdot -2) + (-1 \\cdot 2) = 2"
        },
        {
            "text": "\\Vert A \\Vert \\Vert B \\Vert \\cos \\theta &=& 2"
        },
        {
            "text": "\\sqrt{6} \\sqrt{9} \\cos \\theta &=& 2"
        },
        {
            "text": "\\theta &=& 74.2\\degree"
        },
        {
            "text": "\\end{eqnarray*}"
        },
        {
            "text": "\\begin{eqnarray*}"
        },
        {
            "text": "\\overrightarrow{C} &=& \\overrightarrow{A} \\times \\overrightarrow{B} "
        },
        {
            "text": "&=&"
        },
        {
            "text": "\\left|"
        },
        {
            "text": "\\begin{matrix}"
        },
        {
            "text": "1 & -1 "
        },
        {
            "text": "-2 & 2"
        },
        {
            "text": "\\end{matrix}"
        },
        {
            "text": "\\right| i -"
        },
        {
            "text": "\\left|"
        },
        {
            "text": "\\begin{matrix}"
        },
        {
            "text": "2 & -1 "
        },
        {
            "text": "1 & 2"
        },
        {
            "text": "\\end{matrix} \\right| j +"
        },
        {
            "text": "\\left|"
        },
        {
            "text": "\\begin{matrix}"
        },
        {
            "text": "2 & 1 "
        },
        {
            "text": "1 & -2"
        },
        {
            "text": "\\end{matrix}"
        },
        {
            "text": "\\right| k"
        },
        {
            "text": "&=& 0 i - 5 j - 5 k "
        },
        {
            "text": "&& \\text{berada di bidang $yz$}"
        },
        {
            "text": "\\end{eqnarray*}"
        }
    ],
